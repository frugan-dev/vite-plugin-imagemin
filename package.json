{
  "name": "vite-plugin-imagemin",
  "version": "1.0.0",
  "description": "A vite plugin for compressing image assets",
  "author": "Philip van Heemstra",
  "license": "ISC",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "watch": "tsup src/index.ts --format esm --dts --clean --watch src",
    "build-all": "tsup src/index.ts --format esm --dts --clean && pnpm --filter playground build",
    "build-all-inspect": "tsup src/index.ts --format esm --dts --clean && pnpm --filter playground build-inspect",
    "prepublishOnly": "npm run build",
    "unsupported_double_build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "_stub": "pnpm unbuild --stub",
    "_stub-all": "pnpm unbuild --stub && pnpm --filter playground build",
    "_stub-all-inspect": "pnpm unbuild --stub && pnpm --filter playground build-inspect",
    "_build": "pnpm unbuild",
    "_build-all": "pnpm unbuild && pnpm --filter playground build",
    "_build-all-inspect": "pnpm unbuild && pnpm --filter playground build-inspect",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "imagemin",
    "minify",
    "compress",
    "jpg",
    "png",
    "gif",
    "svg",
    "webp",
    "avif"
  ],
  "homepage": "https://github.com/vheemstra/vite-plugin-imagemin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vheemstra/vite-plugin-imagemin.git"
  },
  "bugs": {
    "url": "https://github.com/vheemstra/vite-plugin-imagemin/issues"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@vheemstra/imagemin-avifenc": "^2.0.1",
    "chalk": "^5.2.0",
    "fs-extra": "^11.1.0",
    "imagemin": "^8.0.1",
    "imagemin-avif": "^0.1.4",
    "imagemin-gif2webp": "^3.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegoptim": "^8.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^7.0.0",
    "vite": "^4.0.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2"
  }
}
